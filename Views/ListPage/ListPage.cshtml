    function showDataTableOperation() 
    {
            var scrollLeft=$(".DataTable-Container").scrollLeft();
            var width=$(".DataTable-Container").width();
            var operationWidth=$(".DataTable-Operation").width();
            var left=width-operationWidth+scrollLeft;
            var scrollWidth=$(".DataTable-Container")[0].scrollWidth;
            var clientWidth=$(".DataTable-Container")[0].clientWidth;
            console.log("scrollWidth="+scrollWidth);
            console.log("clientWidth="+clientWidth);
            console.log("left="+left);
            if(scrollWidth!=clientWidth)
            {
                $(".DataTable-Operation").show();
                $(".DataTable-Operation").css("left",left+"px");
            }
            else
            {
                $(".DataTable-Operation").hide();
            }
            console.log(scrollLeft);
    }
</script>
