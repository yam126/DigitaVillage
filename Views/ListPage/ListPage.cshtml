<script type="text/javascript">
    var CurPageNumberIndex=1;
    $(document).ready(function(){
        $("#ListPageMain").click(function(){
          $(".SecondMenuFoldPopMenu").hide();
        });
        $("#PageContainerCenter .pageNumber-Container").children().each(function(index,element){
            var pageNumberLength=$("#PageContainerCenter .pageNumber-Container").children().length;
            $(element).click(function(){
                if(index!=0&&index!=pageNumberLength-1){
                    CurPageNumberIndex=index;
                    $("#PageContainerCenter .pageNumber-Container").children().each(function(index,element){
                       if(index==CurPageNumberIndex)
                          $(element).attr("class","pageNumber-Click-Item");
                       else
                          $(element).attr("class","pageNumber-Item");
                    });
                }
            });
        });
    });
</script>
}
